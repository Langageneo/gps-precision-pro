{
  "manifest_version": 3,
  "name": "GPS Precision — Livraison",
  "version": "1.0",
  "description": "Extension pour correcteur GPS et affichage itinéraire",
  "permissions": ["geolocation", "storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["http://localhost:8000/*"]
}
